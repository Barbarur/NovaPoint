<Page x:Class="NovaPointWPF.Pages.Solutions.Report.MembershipReportForm"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NovaPointWPF.Pages.Solutions.Report"
      xmlns:designmaterial="clr-namespace:NovaPointWPF.UserControls"
      mc:Ignorable="d" 
      d:DesignHeight="800" d:DesignWidth="800"
      Title="MembershipReportForm">

    <Grid Margin="0,0,0,16">

        <StackPanel>

            <designmaterial:MembershipForm
                x:Name="MembershipF"/>

            <designmaterial:SiteTenantForm
            x:Name="SiteF"/>

            <designmaterial:AdminForm
            x:Name="AdminF"/>

        </StackPanel>

    </Grid>
</Page>
